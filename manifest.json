{
  "name": "Personalised Site Blocker",
  "description": "Chrome extension to hide any site I want",
  "version": "2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
     "storage",
     "activeTab",
     "scripting",
     "tabs"
   ],
  "host_permissions": [
        "https://www.espncricinfo.com/*",
        "https://www.youtube.com/*",
        "https://www.cwseed.com/*"
   ]
}
